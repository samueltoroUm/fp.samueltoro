#lista de 0 a 20, que cargue aleatoriamente, parametros de la funcion 
import random
lista1=[]
for x in range (1,20):
    numero=random.randint (1,20)
    lista1.append(numero);
print (lista1)
#2
busNum= int (input("escribir el numero a buscar: "))
for x in range (0,20):
    if lista1[x]==busNum:
     lista1[x]=-1
     print ("la lista actualizada con el valor cambiado a -1", lista1)
